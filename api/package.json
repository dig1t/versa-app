{
  "name": "versa-api",
  "version": "1.0.0",
  "description": "Versa API",
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "nodemonConfig": {
    "delay": 100
  },
  "scripts": {
    "build": "swc src -D -d dist",
    "dev:compile": "set NODE_ENV=production&& swc src -w -D -d dist",
    "dev:watch": "nodemon --watch \"dist/**/*\" --exec \"node --no-warnings ./dist/index.js\"",
    "dev": "concurrently \"yarn run dev:compile\" \"yarn run dev:watch\"",
    "start": "set NODE_ENV=production&& node ./dist/index.js",
    "test": "set NODE_ENV=development&& mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dig1t/versa-app.git"
  },
  "author": "dig1t",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dig1t/versa-app/issues"
  },
  "homepage": "https://github.com/dig1t/versa-app#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.319.0",
    "@aws-sdk/s3-request-presigner": "^3.319.0",
    "@swc-node/register": "^1.6.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.54",
    "@swc/helpers": "^0.5.1",
    "@swc/register": "^0.1.10",
    "aws-sdk": "^2.1364.0",
    "bcrypt": "^5.1.0",
    "bottleneck": "^2.19.5",
    "compression": "^1.7.4",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.1.5",
    "mongoose": "^7.0.5",
    "multer": "^1.4.5-lts.1",
    "oidc-provider": "^8.2.0",
    "swc-loader": "^0.2.3",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^2.0.22"
  }
}