{
  "name": "versa-api",
  "version": "1.0.0",
  "description": "Versa API",
  "private": true,
  "main": "src/index.js",
  "type": "module",
  "nodemonConfig": {
    "delay": 100
  },
  "scripts": {
    "build": "swc src -D -d dist",
    "make-client": "node --no-warnings ./config/createClient.js",
    "dev:compile": "set NODE_ENV=production&& swc src -w -D -d dist",
    "dev:watch": "nodemon --watch \"dist/**/*\" --exec \"node --no-warnings ./dist/index.js\"",
    "dev": "set NODE_ENV=development&& node --no-warnings ./src/index.js",
    "start": "set NODE_ENV=production&& node ./dist/index.js",
    "test": "set NODE_ENV=development&& mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dig1t/versa-app.git"
  },
  "author": "dig1t",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dig1t/versa-app/issues"
  },
  "homepage": "https://github.com/dig1t/versa-app#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.350.0",
    "@aws-sdk/lib-storage": "^3.350.0",
    "@swc-node/register": "^1.6.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.1",
    "@swc/register": "^0.1.10",
    "bcrypt": "^5.1.0",
    "bottleneck": "^2.19.5",
    "busboy": "^1.6.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "mongoose": "^7.2.4",
    "node-jose": "^2.2.0",
    "npm-check-updates": "^16.10.12",
    "oidc-provider": "^8.2.2",
    "swc-loader": "^0.2.3",
    "validator": "^13.9.0",
    "webpack": "^5.86.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.2",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^2.0.22"
  }
}
